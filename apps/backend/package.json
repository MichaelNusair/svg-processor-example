{
  "name": "@svg-processor/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Backend API for SVG file processing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
